# Requires Julia files to be run first
tib<-read_csv("sim0.csv")
names(tib)<-c("dist","N","B", "Quantile")
tib$Quantile<-as_factor(tib$Quantile)
cbPalette <- c("#000000", "#E69F00", "#56B4E9", "#009E73", "#F0E442", "#0072B2", "#D55E00", "#CC79A7")

ggplot(tib, aes(N,dist,group=Quantile))+geom_line(aes(color=Quantile))+geom_point(aes(shape=Quantile))+geom_hline(yintercept = 0)+scale_y_continuous("KS-statistic")+scale_colour_manual(values=cbPalette)
ggsave("fig_mc_KS.pdf", width = 6, height = 6*0.62)

