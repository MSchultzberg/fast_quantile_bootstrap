<<echo=FALSE, cache=TRUE, message=FALSE, warning=FALSE>>=
# EMPIRICAL BOOTSTRAP DISTRIBUTION
set.seed(4321)
N<-2000
Q=c(0.1,0.5)
B<-1000000
ordered_x = 1:N
index_med<-matrix(NA,B,2)
for(b in 1:B){
  pois_temp=rpois(N,1)
  n<-round((sum(pois_temp)+1)*Q)
  index_med[b,]<-ifelse(round(n)==0,0,rep(ordered_x, pois_temp)[n])
}

@



